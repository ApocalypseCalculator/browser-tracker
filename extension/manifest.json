{
    "manifest_version": 3,
    "name": "Browser Tracker",
    "description": "Monitor browsing activity through remote reporting",
    "version": "1.0.0",
    "author": "ApocalypseCalculator",
    "permissions": [
        "alarms",
        "background",
        "history",
        "storage"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": []
}